<h1>Listing secondary_tbls</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Height</th>
      <th>Width</th>
      <th>Distance starts</th>
      <th>Distance ends</th>
      <th>Masters</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @secondary_tbls.each do |secondary_tbl| %>
      <tr>
        <td><%= secondary_tbl.name %></td>
        <td><%= secondary_tbl.description %></td>
        <td><%= secondary_tbl.height %></td>
        <td><%= secondary_tbl.width %></td>
        <td><%= secondary_tbl.distance_starts %></td>
        <td><%= secondary_tbl.distance_ends %></td>
        <td><%= secondary_tbl.masters_id %></td>
        <td><%= link_to 'Show', secondary_tbl %></td>
        <td><%= link_to 'Edit', edit_secondary_tbl_path(secondary_tbl) %></td>
        <td><%= link_to 'Destroy', secondary_tbl, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Secondary tbl', new_secondary_tbl_path %>
